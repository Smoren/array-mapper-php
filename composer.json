{
  "name": "smoren/array-mapper",
  "description": "Helper for mapping arrays",
  "keywords": ["map", "array"],
  "license": "MIT",
  "authors": [
    {
      "name": "Smoren",
      "email": "ofigate@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "smoren/extended-exceptions": "^1.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Smoren\\ArrayMapper\\": "src",
      "Smoren\\ArrayMapper\\Tests\\Unit\\": "tests/unit"
    }
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
